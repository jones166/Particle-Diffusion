Cell[BoxData[
 RowBox[{
  RowBox[{"drawPath2D", "[", 
   RowBox[{"a_", ",", "num_", ",", "c_", ",", "l_", ",", "w_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "R", ",", "\[Sigma]", ",", "width", ",", "height", ",", "objectX", ",", 
      "objectY", ",", "V", ",", "\[Lambda]", ",", "collisions", ",", 
      "pathLength", ",", "pathAngle", ",", "coordinates", ",", "index", ",", 
      "newX", ",", "newY", ",", "distance"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"R", "=", 
      RowBox[{
       RowBox[{"ElementData", "[", 
        RowBox[{"a", ",", "\"\<AtomicRadius\>\""}], "]"}], "/", 
       NamespaceBox["LinguisticAssistant",
        DynamicModuleBox[{Typeset`query$$ = "um", Typeset`boxes$$ = 
         TemplateBox[{
           InterpretationBox["\[InvisibleSpace]", 1], "\"\[Mu]m\"", 
           "micrometers", "\"Micrometers\""}, "Quantity", SyntaxForm -> Mod], 
         Typeset`allassumptions$$ = {{
          "type" -> "Clash", "word" -> "um", "template" -> 
           "Assuming \"um\" is a unit${separator}Use as ${desc} instead", 
           "count" -> "2", "pulldown" -> "false", "default" -> 
           "{\"C\", \"um\"} -> {\"Unit\", \"dflt\"}", 
           "Values" -> {{
             "name" -> "Unit", "desc" -> "a unit", "input" -> 
              "{\"C\", \"um\"} -> {\"Unit\"}"}, {
             "name" -> "Word", "desc" -> "a word", "input" -> 
              "{\"C\", \"um\"} -> {\"Word\"}"}}}}, Typeset`assumptions$$ = {},
          Typeset`open$$ = {1, 2}, Typeset`querystate$$ = {
         "Online" -> True, "Allowed" -> True, "mparse.jsp" -> 
          0.9163923`7.413626424862783, "Messages" -> {}}}, 
         DynamicBox[ToBoxes[
           AlphaIntegration`LinguisticAssistantBoxes["", 4, Automatic, 
            Dynamic[Typeset`query$$], 
            Dynamic[Typeset`boxes$$], 
            Dynamic[Typeset`allassumptions$$], 
            Dynamic[Typeset`assumptions$$], 
            Dynamic[Typeset`open$$], 
            Dynamic[Typeset`querystate$$]], StandardForm],
          ImageSizeCache->{95.8, {13., 18.8}},
          
          TrackedSymbols:>{
           Typeset`query$$, Typeset`boxes$$, Typeset`allassumptions$$, 
            Typeset`assumptions$$, Typeset`open$$, Typeset`querystate$$}],
         DynamicModuleValues:>{},
         UndoTrackedVariables:>{Typeset`open$$}],
        BaseStyle->{"Deploy"},
        DeleteWithContents->True,
        Editable->False,
        SelectWithContents->True]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Sigma]", "=", 
      RowBox[{"Pi", " ", 
       RowBox[{"R", "^", "2"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"width", "=", "w"}], ";", "\[IndentingNewLine]", 
     RowBox[{"height", "=", "l"}], ";", "\[IndentingNewLine]", 
     RowBox[{"objectX", "=", 
      RowBox[{"width", "/", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"objectY", "=", 
      RowBox[{"height", "/", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"V", "=", 
      RowBox[{"width", " ", "height", " ", 
       RowBox[{"(", 
        RowBox[{"2", " ", "R"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{"num", "/", "V"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Lambda]", "=", 
      RowBox[{"1", "/", 
       RowBox[{"(", 
        RowBox[{"n", " ", "\[Sigma]"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"collisions", "=", "c"}], ";", "\[IndentingNewLine]", 
     RowBox[{"pathLength", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"pathLength", "=", 
        RowBox[{"Append", "[", 
         RowBox[{"pathLength", ",", 
          RowBox[{"\[Lambda]", "+", 
           RowBox[{"\[Lambda]", " ", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], "^", 
             RowBox[{"RandomInteger", "[", "1", "]"}]}], " ", 
            RowBox[{"(", 
             RowBox[{"E", "^", 
              RowBox[{"(", 
               RowBox[{"-", 
                RowBox[{"RandomReal", "[", 
                 RowBox[{"{", 
                  RowBox[{".25", ",", "4"}], "}"}], "]"}]}], ")"}]}], 
             ")"}]}]}]}], "]"}]}], ",", "collisions"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pathAngle", "=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{
        RowBox[{"2", " ", "Pi"}], ",", "collisions"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"coordinates", "=", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{"objectX", ",", "objectY"}], "}"}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"index", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"newX", "=", 
         RowBox[{
          RowBox[{"coordinates", "[", 
           RowBox[{"[", 
            RowBox[{"index", ",", "1"}], "]"}], "]"}], "+", 
          RowBox[{
           RowBox[{"pathLength", "[", 
            RowBox[{"[", "index", "]"}], "]"}], " ", 
           RowBox[{"Cos", "[", 
            RowBox[{"pathAngle", "[", 
             RowBox[{"[", "index", "]"}], "]"}], "]"}]}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"newY", "=", 
         RowBox[{
          RowBox[{"coordinates", "[", 
           RowBox[{"[", 
            RowBox[{"index", ",", "2"}], "]"}], "]"}], "+", 
          RowBox[{
           RowBox[{"pathLength", "[", 
            RowBox[{"[", "index", "]"}], "]"}], " ", 
           RowBox[{"Sin", "[", 
            RowBox[{"pathAngle", "[", 
             RowBox[{"[", "index", "]"}], "]"}], "]"}]}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"coordinates", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"coordinates", ",", 
           RowBox[{"{", 
            RowBox[{"newX", ",", "newY"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"index", "++"}], ";"}], ",", "\[IndentingNewLine]", 
       "collisions"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"distance", "=", 
      RowBox[{"EuclideanDistance", "[", 
       RowBox[{
        RowBox[{"coordinates", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"coordinates", "[", 
         RowBox[{"[", 
          RowBox[{"collisions", "+", "1"}], "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Graphics", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"Black", ",", 
             RowBox[{"Line", "[", "coordinates", "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"Red", ",", 
             RowBox[{"Line", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"coordinates", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], ",", 
                RowBox[{"coordinates", "[", 
                 RowBox[{"[", 
                  RowBox[{"collisions", "+", "1"}], "]"}], "]"}]}], "}"}], 
              "]"}]}], "}"}]}], "}"}], ",", 
         RowBox[{"Frame", "\[Rule]", "True"}], ",", 
         RowBox[{"PlotRange", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"0", ",", "width"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "height"}], "}"}]}], "}"}]}]}], "]"}], ",", 
       "distance"}], "}"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
