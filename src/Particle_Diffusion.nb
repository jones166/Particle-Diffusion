Cell["2D draw path function", "Text",
 CellChangeTimes->{{3.8588807381135054`*^9, 
  3.858880743118106*^9}},ExpressionUUID->"80698e42-f875-48ad-85d3-\
74d4b7e1e8dd"],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawPath2D", "[", 
   RowBox[{"a_", ",", "num_", ",", "c_", ",", "l_", ",", "w_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "m", ",", "R", ",", "simR", ",", "\[Sigma]", ",", "width", ",", "height",
       ",", "objectX", ",", "objectY", ",", "V", ",", "\[Lambda]", ",", 
      "collisions", ",", "pathLength", ",", "pathAngle", ",", "coordinates", 
      ",", "index", ",", "newX", ",", "newY"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"m", "=", 
      RowBox[{"ElementData", "[", 
       RowBox[{"atom", ",", "\"\<AtomicMass\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"m", "=", 
      RowBox[{"UnitConvert", "[", 
       RowBox[{"m", ",", "\"\<MeV/c^2\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"R", "=", 
      RowBox[{"ElementData", "[", 
       RowBox[{"a", ",", "\"\<AtomicRadius\>\""}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"simR", "=", 
      RowBox[{
       RowBox[{"QuantityMagnitude", "[", "R", "]"}], "/", "1000"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Sigma]", "=", 
      RowBox[{"Pi", " ", 
       RowBox[{"R", "^", "2"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"width", "=", 
      RowBox[{"Quantity", "[", 
       RowBox[{"w", ",", "\"\<um\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"height", "=", 
      RowBox[{"Quantity", "[", 
       RowBox[{"l", ",", "\"\<um\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"objectX", "=", 
      RowBox[{"width", "/", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"objectY", "=", 
      RowBox[{"height", "/", "2"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"V", "=", 
      RowBox[{"width", " ", "height", " ", 
       RowBox[{"(", 
        RowBox[{"2", " ", "R"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{"num", "/", "V"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Lambda]", "=", 
      RowBox[{"1", "/", 
       RowBox[{"(", 
        RowBox[{"n", " ", "\[Sigma]"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"collisions", "=", "c"}], ";", "\[IndentingNewLine]", 
     RowBox[{"pathLength", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"pathLength", "=", 
        RowBox[{"Append", "[", 
         RowBox[{"pathLength", ",", 
          RowBox[{"\[Lambda]", "+", 
           RowBox[{"\[Lambda]", " ", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"-", "1"}], ")"}], "^", 
             RowBox[{"RandomInteger", "[", "1", "]"}]}], " ", 
            RowBox[{"(", 
             RowBox[{"E", "^", 
              RowBox[{"(", 
               RowBox[{"-", 
                RowBox[{"RandomReal", "[", 
                 RowBox[{"{", 
                  RowBox[{".25", ",", "4"}], "}"}], "]"}]}], ")"}]}], 
             ")"}]}]}]}], "]"}]}], ",", "collisions"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pathAngle", "=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{
        RowBox[{"2", " ", "Pi"}], ",", "collisions"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"coordinates", "=", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{"objectX", ",", "objectY"}], "}"}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"index", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"newX", "=", 
         RowBox[{
          RowBox[{"coordinates", "[", 
           RowBox[{"[", 
            RowBox[{"index", ",", "1"}], "]"}], "]"}], "+", 
          RowBox[{
           RowBox[{"pathLength", "[", 
            RowBox[{"[", "index", "]"}], "]"}], " ", 
           RowBox[{"Cos", "[", 
            RowBox[{"pathAngle", "[", 
             RowBox[{"[", "index", "]"}], "]"}], "]"}]}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"newY", "=", 
         RowBox[{
          RowBox[{"coordinates", "[", 
           RowBox[{"[", 
            RowBox[{"index", ",", "2"}], "]"}], "]"}], "+", 
          RowBox[{
           RowBox[{"pathLength", "[", 
            RowBox[{"[", "index", "]"}], "]"}], " ", 
           RowBox[{"Sin", "[", 
            RowBox[{"pathAngle", "[", 
             RowBox[{"[", "index", "]"}], "]"}], "]"}]}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"coordinates", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"coordinates", ",", 
           RowBox[{"{", 
            RowBox[{"newX", ",", "newY"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"index", "++"}], ";"}], ",", "\[IndentingNewLine]", 
       "collisions"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"Black", ",", 
           RowBox[{"Line", "[", 
            RowBox[{"QuantityMagnitude", "[", 
             RowBox[{"UnitConvert", "[", 
              RowBox[{"coordinates", ",", "\"\<um\>\""}], "]"}], "]"}], 
            "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"Red", ",", 
           RowBox[{"Line", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"QuantityMagnitude", "[", 
               RowBox[{"coordinates", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "]"}], ",", 
              RowBox[{"QuantityMagnitude", "[", 
               RowBox[{"coordinates", "[", 
                RowBox[{"[", 
                 RowBox[{"collisions", "+", "1"}], "]"}], "]"}], "]"}]}], 
             "}"}], "]"}]}], "}"}]}], "}"}], ",", 
       RowBox[{"Frame", "\[Rule]", "True"}], ",", 
       RowBox[{"PlotRange", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", 
            RowBox[{"QuantityMagnitude", "[", "width", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", 
            RowBox[{"QuantityMagnitude", "[", "height", "]"}]}], "}"}]}], 
         "}"}]}]}], "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
